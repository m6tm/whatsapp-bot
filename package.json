{
  "name": "whatsapp-bot",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "app_env": "local",
  "scripts": {
    "start": "node www/src/app.js",
    "build": "rm -rf www/* && tsc && cp -r views www && cp package.json www && node www/src/app.js",
    "dev": "ts-node src/app.ts",
    "watch-poll": "nodemon --exec ts-node src/app.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "in_array": "^1.1.0",
    "morgan": "~1.9.1",
    "path": "^0.12.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.1",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
